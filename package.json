{
  "name": "p3-web",
  "version": "3.5.1",
  "private": true,
  "scripts": {
    "dev": "gulp srcfiles && npm run moveuser && npm run browserify && npm run redis && npm start",
    "start": "node ./bin/p3-web",
    "start:redis": "redis-server",
    "redis": "gnome-terminal -e \"bash -c 'npm run start:redis;bash'\"\n",
    "stop:redis": "/etc/init.d/redis-server stop",
    "installglobals": "npm install -g yarn@latest && npm install -g gulp@latest",
    "browserify": "browserify ./gulpified/regmain.js -o public/js/regmain.js && browserify ./gulpified/userutilmain.js -o public/js/userutilmain.js && browserify ./gulpified/useraccountmain.js -o public/js/useraccountmain.js",
    "buildClient": "./buildClient.sh",
    "buildBack": "./buildBack.sh",
    "moveuser": "cp src/styles.css public/js/ && cp src/userutil/index.html public/userutil/ && cp src/useraccount/index.html public/useraccount/",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json --notify",
    "eslint-fix": "./node_modules/eslint/bin/eslint.js --fix ."
  },
  "dependencies": {
    "aurelia-tools": "^1.0.0",
    "babel-eslint": "^7.2.3",
